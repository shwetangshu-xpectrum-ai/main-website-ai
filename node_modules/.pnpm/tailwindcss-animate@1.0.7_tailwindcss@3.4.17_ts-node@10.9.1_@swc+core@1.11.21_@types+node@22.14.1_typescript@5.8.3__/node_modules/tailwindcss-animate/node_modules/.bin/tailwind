#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shwetangshu/Downloads/Subhankr Work/main-website-karthikeya/node_modules/.pnpm/tailwindcss@3.4.17_ts-node@10.9.1_@swc+core@1.11.21_@types+node@22.14.1_typescript@5.8.3_/node_modules/tailwindcss/lib/node_modules:/Users/shwetangshu/Downloads/Subhankr Work/main-website-karthikeya/node_modules/.pnpm/tailwindcss@3.4.17_ts-node@10.9.1_@swc+core@1.11.21_@types+node@22.14.1_typescript@5.8.3_/node_modules/tailwindcss/node_modules:/Users/shwetangshu/Downloads/Subhankr Work/main-website-karthikeya/node_modules/.pnpm/tailwindcss@3.4.17_ts-node@10.9.1_@swc+core@1.11.21_@types+node@22.14.1_typescript@5.8.3_/node_modules:/Users/shwetangshu/Downloads/Subhankr Work/main-website-karthikeya/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/shwetangshu/Downloads/Subhankr Work/main-website-karthikeya/node_modules/.pnpm/tailwindcss@3.4.17_ts-node@10.9.1_@swc+core@1.11.21_@types+node@22.14.1_typescript@5.8.3_/node_modules/tailwindcss/lib/node_modules:/Users/shwetangshu/Downloads/Subhankr Work/main-website-karthikeya/node_modules/.pnpm/tailwindcss@3.4.17_ts-node@10.9.1_@swc+core@1.11.21_@types+node@22.14.1_typescript@5.8.3_/node_modules/tailwindcss/node_modules:/Users/shwetangshu/Downloads/Subhankr Work/main-website-karthikeya/node_modules/.pnpm/tailwindcss@3.4.17_ts-node@10.9.1_@swc+core@1.11.21_@types+node@22.14.1_typescript@5.8.3_/node_modules:/Users/shwetangshu/Downloads/Subhankr Work/main-website-karthikeya/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../tailwindcss@3.4.17_ts-node@10.9.1_@swc+core@1.11.21_@types+node@22.14.1_typescript@5.8.3_/node_modules/tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../../../tailwindcss@3.4.17_ts-node@10.9.1_@swc+core@1.11.21_@types+node@22.14.1_typescript@5.8.3_/node_modules/tailwindcss/lib/cli.js" "$@"
fi
